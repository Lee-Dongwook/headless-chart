---
import Layout from "./Layout.astro";
interface Props {
  title?: string;
  description?: string;
  image?: string;
}

const { title, description, image } = Astro.props;
---

<Layout title={`${title} - Headless Charts`}description={description} image={image ?? "/og-docs.png"}>
  <slot />
</Layout>

<style is:global>
  text {
    user-select: none;
  }
</style>
